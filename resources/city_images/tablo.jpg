        <script>
            r(function() {
                $('.product-grid .product-item a').click(function(e) {

                    var productId = $(this).parents('.product-item').data('productid');
                    var url = $(this).attr('href');

                    var product;
                    for (var i = 0; i < impressions.length; i++) {
                        if (impressions[i].id === productId) {
                            product = impressions[i];
                        }
                    }

                    console.log('gtm plugin product click', productId, url);

                    if (product) {
                        dataLayer.push({
                            'event': 'productClick',
                            'ecommerce': {
                                'click': {
                                    'actionField': { 'list': product.list },
                                    'products': [product]
                                }
                            },
                            'eventCallback': function() {
                                console.log('gtm plugin product click callback');
                                //document.location = url;
                            }
                        });
                    }
                });
            });
        </script>
    <script>
        dataLayer.push({ 'ecommerce': ecommerceLayer });
